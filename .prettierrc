useTabs: false     
printWidth: 80     
tabWidth: 2
singleQuote: true 
trailingComma: all
bracketSpacing: true
jsxBracketSameLine: false
semi: false
arrowParens: always
overrides:
 - {files: "*.less", options: {parser: "less"}}
 - {files: "*.scss", options: {parser: "scss"}}
 - {files: "*.js", options: {parser: "flow"}}

